/*
https://codeforces.com/problemset/problem/1296/A
*/
#include<iomanip>
#include<iostream>
#include<bits/stdc++.h>
int main()
{
int t;
std::cin>>t;
while(t--)
{
int n,i,sum=0;
std::cin>>n;
int a[n];
for(i=0;i<n;i++)
std::cin>>a[i];
for(i=0;i<n;i++)
sum=sum+a[i];
if(sum%2==0)
std::cout<<"no";
else 
std::cout<<"yes";
std::cout<<std::endl;
}
return 0;
}
